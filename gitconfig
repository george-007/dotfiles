[core]
	editor = vim +1 +start
[user]
	name = Jiri Pejchal
	email = jiri.pejchal@gmail.com
[push]
	default = simple
[alias]
	ci = commit -v
	co = checkout
	st = status
        up = rebase
        lol = log --graph --decorate --pretty=oneline --abbrev-commit
        lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	type = cat-file -t
	dump = cat-file -p
[interactive]
        singlekey = true
[apply]
	whitespace = fix

