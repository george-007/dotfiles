[core]
	editor = vim +1 +start
[push]
	default = simple
[alias]
	st = status
	ci = commit --verbose
	co = checkout
    di = diff
    dc = diff --cached
    ff = merge --ff-only
    pulff = pull --ff-only
    fa = fetch --all
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    pom = push origin master
    b = branch
	type = cat-file -t
	dump = cat-file -p
    up = pull --rebase --prune
[interactive]
     singlekey = true
[apply]
	whitespace = fix
[branch]
    autosetuprebase = always
[include]
    path = ~/.gitconfig.local
# vim: set ft=gitconfig:
